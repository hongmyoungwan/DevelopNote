	implementation('com.esi.nextgen.common_util:kefci-common-nextgen:+') {
		changing = true
		exclude group: 'org.springframework.boot', module: 'spring-boot-starter-web'
		exclude group: 'org.springframework.boot', module: 'spring-boot-starter-webflux'
	}
	implementation('com.esi.nextgen.common_mapper:esi-common-nextgen:+') {
        changing = true
    }


+를 붙힌다

// 이슈 발생 vscode와 , intelliJ는 build 주체가 달라 작동 안될 수 있음
